{
  "name": "iapyang-vue-template",
  "version": "0.2.2",
  "description": "a simple vue template",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --progress --config ./conf/webpack.dev.config.js",
    "build": "webpack --progress --config ./conf/webpack.build.config.js",
    "bundle": "webpack --progress --config ./conf/webpack.bundle.config.js",
    "gh": "node conf/deploy",
    "check": "ncu",
    "update-json": "ncu -a",
    "update-package": "npm update"
  },
  "author": {
    "name": "iapyang",
    "email": "yklorse@gmail.com"
  },
  "bin": {
    "iapvt": "./bin/iapvt.js"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "keywords": [
    "vue",
    "vue template",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iapYang/iapyang-vue-template"
  },
  "license": "ISC",
  "devDependencies": {
    "vue-simple-loading": "^1.1.5",
    "vue-video-module": "^0.4.3"
  },
  "dependencies": {
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-html": "2.0.1",
    "file-loader": "0.11.1",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.28.0",
    "node-sass": "4.5.2",
    "postcss-css-variables": "0.7.0",
    "postcss-cssnext": "2.10.0",
    "postcss-loader": "1.3.3",
    "sass-loader": "6.0.3",
    "shelljs": "0.7.7",
    "style-loader": "0.17.0",
    "url-loader": "0.5.8",
    "vue": "2.3.1",
    "vuex": "2.3.1",
    "vue-loader": "12.0.2",
    "vue-router": "2.5.3",
    "vue-template-compiler": "2.3.1",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5",
    "webpack-merge": "4.1.0"
  }
}
