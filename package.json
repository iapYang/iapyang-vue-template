{
  "name": "iapyang-vue-template",
  "version": "0.5.5",
  "description": "a simple vue template",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./conf/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config ./conf/webpack.build.config.js",
    "bundle": "webpack --progress --config ./conf/webpack.bundle.config.js",
    "gh": "node ./bin/iapvt.js gh",
    "cli": "node ./bin/iapvt-cli.js",
    "check": "ncu",
    "update-json": "ncu -a",
    "update-package": "npm update"
  },
  "author": {
    "name": "iapyang",
    "email": "yklorse@gmail.com"
  },
  "bin": {
    "iapvt": "./bin/iapvt.js"
  },
  "keywords": [
    "vue",
    "vue template",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iapYang/iapyang-vue-template"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.1.1",
